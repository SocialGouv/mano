{
  "name": "mano",
  "version": "2.8.0",
  "private": true,
  "scripts": {
    "get-ip": "./get-ip.sh",
    "android": "npm run get-ip && react-native run-android",
    "ios": "npm run get-ip && react-native run-ios",
    "start": "react-native start --reset-cache",
    "test": "jest --watch",
    "lint": "eslint .",
    "detox-build": "rm -rf ios/build && detox build --configuration ios",
    "detox-test": "echo -p \"please npm run start NOWWW\" && detox test ./e2e --reuse --configuration ios",
    "detox-build-and-test": "npm run detox-build && npm run detox-test",
    "android-clean-start": "npm run clean:android && react-native run-android",
    "postinstall": "pod install --project-directory=ios/ && npx jetify",
    "clean:android": "cd android && ./gradlew clean && cd ../ && adb uninstall com.mano",
    "build:android": "cd android && ./gradlew clean && ./gradlew bundleRelease && cd ../",
    "build:android-apk": "cd android && ./gradlew clean && ./gradlew assembleRelease && cd ../",
    "build:android-apk-preproduction": "echo -p \"please update either .env.production with the preproduction values until we found a better solution ðŸ¥¸\" && cd android && ./gradlew clean && ./gradlew assembleRelease && cd ../",
    "deploy:android": "./upload.sh",
    "build-and-deploy:android": "npm run build:android-apk && ./upload.sh",
    "build-and-deploy-preprod:android": "npm run build:android-apk-preproduction && ./upload-preprod.sh",
    "update-mobile-app-version": "node ./update-mobile-app-version.js"
  },
  "dependencies": {
    "@expo/react-native-action-sheet": "^3.9.0",
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/datetimepicker": "^3.4.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/netinfo": "^6.0.0",
    "@react-native-community/progress-bar-android": "^1.0.4",
    "@react-native-community/progress-view": "^1.3.0",
    "@react-native-picker/picker": "^1.12.0",
    "@react-navigation/bottom-tabs": "^5.11.8",
    "@react-navigation/material-top-tabs": "^5.3.14",
    "@react-navigation/native": "^5.9.3",
    "@react-navigation/stack": "^5.14.3",
    "@sentry/react-native": "^2.5.1",
    "base-64": "^1.0.0",
    "base64-js": "^1.5.1",
    "buffer": "^6.0.3",
    "fast-text-encoding": "^1.0.3",
    "fetch-retry": "^5.0.1",
    "install": "^0.13.0",
    "ip-anonymize": "^0.1.0",
    "javascript-time-ago": "^2.3.4",
    "jsonwebtoken": "^8.5.1",
    "libphonenumber-js": "^1.9.13",
    "npm": "^7.19.1",
    "prop-types": "^15.7.2",
    "react": "16.14.0",
    "react-native": "0.64.1",
    "react-native-base64": "^0.2.1",
    "react-native-bootsplash": "^3.2.0",
    "react-native-config": "^1.4.5",
    "react-native-country-picker-modal": "^2.0.0",
    "react-native-device-info": "^8.0.6",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-keychain": "^6.2.0",
    "react-native-mmkv-storage": "^0.6.6",
    "react-native-pager-view": "^5.1.2",
    "react-native-pdf": "^6.3.0",
    "react-native-reanimated": "^2.0.0",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^2.18.1",
    "react-native-sodium": "^0.4.0",
    "react-native-svg": "^12.1.1",
    "react-native-tab-view": "^3.0.0",
