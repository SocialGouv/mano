    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "detox": "^18.18.0",
    "eslint": "7.32.0",
    "jest": "^27.0.4",
    "metro-react-native-babel-preset": "^0.64.0",
    "react-test-renderer": "17.0.2",
    "semver": "^7.3.5"
  },
  "jest": {
    "preset": "react-native",
    "forceCoverageMatch": [
      "**/*.test.js"
    ]
  }
